{
  "multipart": [
    {
      "apply": {
        "model": "fossil:block/reinforced_glass"
      }
    },
    {
      "when": {
        "up": "false"
      },
      "apply": {
        "model": "fossil:block/reinforced_glass_down",
        "uvlock": false
      }
    },
    {
      "when": {
        "down": "false"
      },
      "apply": {
        "model": "fossil:block/reinforced_glass_up",
        "uvlock": false
      }
    },
    {
      "when": {
        "east": "false"
      },
      "apply": {
        "model": "fossil:block/reinforced_glass_left",
        "y": 0,
        "uvlock": false
      }
    },
    {
      "when": {
        "west": "false"
      },
      "apply": {
        "model": "fossil:block/reinforced_glass_right",
        "y": 0,
        "uvlock": false
      }
    },
    {
      "when": {
        "north": "false"
      },
      "apply": {
        "model": "fossil:block/reinforced_glass_right",
        "y": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "south": "false"
      },
      "apply": {
        "model": "fossil:block/reinforced_glass_left",
        "y": 90,
        "uvlock": false
      }
    }
  ]
}